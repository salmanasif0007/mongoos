      <script>
          var baby = document.getElementsByClassName("dropdown-btn");
         
var i;

for (i = 0; i < baby.length; i++) {
  baby[i].addEventListener("click", function() {
    this.classList.toggle("active");
    var babyContent = this.nextElementSibling;
    if (babyContent.style.display === "block") {
      babyContent.style.display = "none";
    } else {
      babyContent.style.display = "block";
    }
  });
}

</script>



