.sidenav {
    height: 100%;
    width: 300px;
    position: fixed;
    z-index: 1;
    top: 0;
    left: 1%;
    background-color: #fff;
    overflow-x: hidden;
    padding-top: 20px;
    border: 1px solid rgb(187, 187, 187);
  }
  .sidenav h4{
      color: black;
      background-color: brown;
      font-size: 20px;
